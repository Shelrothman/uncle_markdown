# Copy this file to .env.local and fill in your values

# GitHub OAuth App credentials
VITE_GITHUB_CLIENT_ID=your_github_oauth_client_id_here
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret_here

# API URL (for local dev, use http://localhost:3001/api/auth/github)
# For production/Vercel, use /api/auth (relative path)
VITE_API_URL=http://localhost:3001/api/auth/github

# App URL
VITE_APP_URL=http://localhost:5173
